        android:text="查看所有订单"
        android:textColor="@color/color_3333"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/textView18" />

    <ImageView
        android:id="@+id/daifu"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="16dp"
        app:layout_constraintEnd_toStartOf="@+id/daifa"
        app:layout_constraintHorizontal_chainStyle="spread_inside"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView18"
        app:srcCompat="@drawable/daifukuai" />

    <ImageView
        android:id="@+id/daifa"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="16dp"
        app:layout_constraintEnd_toStartOf="@+id/daishou"
        app:layout_constraintStart_toEndOf="@+id/daifu"
        app:layout_constraintTop_toTopOf="@+id/daifu"
        app:srcCompat="@drawable/daifahuo" />

    <ImageView
        android:id="@+id/daishou"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="16dp"
        app:layout_constraintEnd_toStartOf="@+id/wancheng"
        app:layout_constraintStart_toEndOf="@+id/daifa"
        app:layout_constraintTop_toTopOf="@+id/daifa"
        app:srcCompat="@drawable/daishouhuo" />

    <ImageView
        android:id="@+id/wancheng"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toTopOf="@+id/daishou_txt"
        app:layout_constraintEnd_toStartOf="@+id/tuikuan"
        app:layout_constraintStart_toEndOf="@+id/daishou"
        app:layout_constraintTop_toTopOf="@+id/daishou"
        app:srcCompat="@drawable/yiwancheng" />

    <ImageView
        android:id="@+id/tuikuan"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="32dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/wancheng"
        app:layout_constraintTop_toTopOf="@+id/wancheng"
        app:srcCompat="@drawable/tuikuan" />

    <TextView
        android:id="@+id/daifu_txt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="待付款"
        android:textColor="@color/color_4e4e"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="@+id/daifu"
        app:layout_constraintStart_toStartOf="@+id/daifu"
        app:layout_constraintTop_toBottomOf="@+id/daifu" />

    <TextView
        android:id="@+id/daifa_txt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="待发货"
        android:textColor="@color/color_4e4e"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="@+id/daifa"
        app:layout_constraintStart_toStartOf="@+id/daifa"
        app:layout_constraintTop_toTopOf="@+id/daifu_txt" />

    <TextView
        android:id="@+id/daishou_txt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="待收货"
        android:textColor="@color/color_4e4e"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="@+id/daishou"
        app:layout_constraintStart_toStartOf="@+id/daishou"
        app:layout_constraintTop_toTopOf="@+id/daifu_txt" />

    <TextView
        android:id="@+id/yiwancheng_txt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="已完成"
        android:textColor="@color/color_4e4e"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="@+id/wancheng"
        app:layout_constraintStart_toStartOf="@+id/wancheng"
        app:layout_constraintTop_toTopOf="@+id/daifu_txt" />

    <TextView
        android:id="@+id/tuikuan_txt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:text="退款/售后"
        android:textColor="@color/color_4e4e"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="@+id/tuikuan"
        app:layout_constraintStart_toStartOf="@+id/tuikuan"
        app:layout_constraintTop_toTopOf="@+id/daifu_txt" />

    <TextView
        android:id="@+id/line1"
        android:layout_width="0dp"
        android:layout_height="6dp"
        android:layout_marginTop="24dp"
        android:background="@color/color_d7d7"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/daifu_txt" />

    <ImageView
        android:id="@+id/wodedianpu"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:layout_marginTop="16dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/line1"
        app:srcCompat="@drawable/wodedianpu" />

    <TextView
        android:id="@+id/wodedianpu_txt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:text="我的店铺"
        android:textColor="@color/color_3333"
        android:textSize="14sp"
        app:layout_constraintBottom_toBottomOf="@+id/wodedianpu"
        app:layout_constraintStart_toEndOf="@+id/wodedianpu"
        app:layout_constraintTop_toTopOf="@+id/wodedianpu" />

    <ImageView
        android:id="@+id/wodedianpu_arror"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="24dp"
        app:layout_constraintBottom_toBottomOf="@+id/wodedianpu"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/wodedianpu"
        app:srcCompat="@drawable/arrow_right_gray" />

    <TextView
        android:id="@+id/line2"
        android:layout_width="0dp"
        android:layout_height="1px"
        android:layout_marginTop="16dp"
        android:background="@color/color_d7d7"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/wodedianpu" />

    <ImageView
        android:id="@+id/dianpuguanli"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:layout_marginTop="16dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/line2"
        app:srcCompat="@drawable/diapuguahli"
        tools:srcCompat="@drawable/diapuguahli" />

    <TextView
        android:id="@+id/dianpuguanli_txt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:text="店铺管理"
        android:textColor="@color/color_3333"
        android:textSize="14sp"
        app:layout_constraintBottom_toBottomOf="@+id/dianpuguanli"
        app:layout_constraintStart_toEndOf="@+id/dianpuguanli"
        app:layout_constraintTop_toTopOf="@+id/dianpuguanli" />

    <TextView
        android:id="@+id/line3"
        android:layout_width="0dp"
        android:layout_height="1px"
        android:layout_marginTop="16dp"
        android:background="@color/color_d7d7"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/dianpuguanli" />

    <ImageView
        android:id="@+id/shiming"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:layout_marginTop="16dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/line3"
        app:srcCompat="@drawable/shiming" />

    <TextView
        android:id="@+id/mystore"
        android:layout_width="0dp"
        android:layout_height="50dp"
        android:background="@android:color/transparent"
        app:layout_constraintBottom_toTopOf="@+id/line2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/line1" />

    <TextView
        android:id="@+id/store_mgr"
        android:layout_width="0dp"
        android:layout_height="50dp"
        android:background="@android:color/transparent"
        app:layout_constraintBottom_toTopOf="@+id/line3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/line2" />

    <TextView
        android:id="@+id/real_name"
        android:layout_width="0dp"
        android:layout_height="50dp"
        android:background="@android:color/transparent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/line3" />

    <TextView
        android:id="@+id/help"
        android:layout_width="0dp"
        android:layout_height="50dp"
        android:background="@android:color/transparent"
        app:layout_constraintBottom_toTopOf="@+id/line6"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/line5" />

    <TextView
        android:id="@+id/hetong"
        android:layout_width="0dp"
        android:layout_height="50dp"
        android:background="@android:color/transparent"
        app:layout_constraintBottom_toTopOf="@+id/line7"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/line6" />


</android.support.constraint.ConstraintLayout>